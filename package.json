{
  "name": "skill-sharing",
  "version": "0.1.0",
  "author": "Falko Schumann",
  "license": "MIT",
  "engines": {
    "node": ">=18.7.0"
  },
  "type": "module",
  "main": "api/main.js",
  "scripts": {
    "start": "node api/main.js",
    "test": "vitest",
    "dev": "npm run \"dev:api\" & npm run \"dev:vite\"",
    "dev:api": "nodemon api/main.ts",
    "dev:vite": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@muspellheim/shared": "0.6.1",
    "bootstrap": "5.3.3",
    "express": "4.21.2",
    "lit-html": "3.2.1",
    "redux": "5.0.1",
    "vite": "6.2.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@types/express": "5.0.1",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "3.0.9",
    "eslint": "9.23.0",
    "eslint-plugin-lit": "2.0.0",
    "eslint-plugin-wc": "3.0.0",
    "eventsource": "3.0.5",
    "jsdom": "26.0.0",
    "nodemon": "3.1.9",
    "prettier": "3.5.3",
    "puppeteer": "24.4.0",
    "rollup": "4.37.0",
    "sass": "1.86.0",
    "supertest": "7.1.0",
    "vitest": "3.0.9"
  },
  "bin": "build/index.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "public/**/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
